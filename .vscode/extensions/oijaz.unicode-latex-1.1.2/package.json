{
	"name": "unicode-latex",
	"displayName": "Unicode Latex",
	"description": "Insert unicode symbols for latex names",
	"version": "1.1.2",
	"publisher": "oijaz",
	"author": {
		"name": "Omer Sheikh"
	},
	"repository": "oijazsh/unicode-latex",
	"engines": {

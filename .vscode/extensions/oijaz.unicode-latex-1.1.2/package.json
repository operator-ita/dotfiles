			{
				"command": "unicode-latex.replaceLatexNames",
				"title": "Unicode: Replace LaTeX"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
